{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "env": { "type": "string", "enum": ["dev"] },
    "enable": { "type": "boolean" },
    "schedule": { "type": "string" },
    "chart": {
      "type": "object",
      "properties": {
        "git": { "type": "boolean" },
        "ref": { "type": "string" }
      },
      "required": ["git", "ref"]
    }
  },
  "required": ["env", "enable", "chart"]
}
