{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "env": { "type": "string" },
    "vaultPrefix": { "type": "string" },
    "cloudsql": {
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "project": { "type": "string" },
        "region": { "type": "string" }
      },
      "required": ["name", "project", "region"]
    }
  },
  "required": ["env", "vaultPrefix", "cloudsql"]
}
