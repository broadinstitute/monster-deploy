{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "vaultPrefix": { "type": "string" },
    "cloudsql": {
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "project": { "type": "string" },
        "region": { "type": "string" }
      },
      "required": ["name", "project", "region"]
    },
    "dns": {
      "type": "object",
      "properties": {
        "zone": { "type": "string" },
        "argo": { "type": "string" },
        "grafana": { "type": "string" }
      },
      "required": ["zone", "argo", "grafana"]
    },
    "stackdriverProject": { "type": "string" },
    "debug": { "type": "boolean" }
  },
  "required": ["vaultPrefix", "cloudsql", "dns", "stackdriverProject", "debug"]
}
