{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "env": { "type": "string", "enum": ["dev"] },
    "cloudsql": {
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "project": { "type": "string" },
        "region": { "type": "string" }
      },
      "required": ["name", "project", "region"]
    },
    "argoNamespaces": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "required": ["env", "cloudsql", "argoNamespaces"]
}
